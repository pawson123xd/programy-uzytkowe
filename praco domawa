int w(int n,int*w){
int m;

m=n;
n=*w;
*w=m;
w=&n;}
int suma(int *a,int*b){
    int const suma=*a+*b;
    return suma;
}
int* mniejsza(int *a,int *b){
    if(*a>*b){
            return b;
    }
    else{
        return a;
}
}
void zmiania2(int *a,int*b){
    printf(" a=%d b=%d\n",*a,*b);
    if(*b<*a){
    int pom=*a;
    *a=*b;
    *b=pom;
    printf(" a=%d b=%d",*a,*b);}
}
int zmiania(int *a,int*b){
    printf(" a=%d b=%d",*a,*b);
    int pom=*a;
    *a=*b;
    *b=pom;
    printf(" a=%d b=%d",*a,*b);
}

int min(int *a,int *b){
    if(*a>*b){
        printf("%d",*b);
    }
    else{
       printf("%d",*a);
    }
}

int fun1(int* a, int* b) { //funkcja przyjmuje dwa wskaÅºniki
    if(*a>*b){
        return *b;
    }
    else {
        return *a;
    }
}
