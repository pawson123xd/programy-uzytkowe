#include <stdio.h>
#include <stdlib.h>
char* strcpy(char*t,const char*s);
int main()
{
    char c[]="Pawels";
    char p[90]; //rozmiar!!
    strcpy(p,c);
    printf("%s",p);
    return 0;
}
char* strcpy(char *t,const char *s){
    for(int i=0;i<*s;i++){
        for(int j=1;j<=1;j++){
            *(t+i)=*(s+i);
        }
        }
    return t;
}
#include <stdio.h>
#include <stdlib.h>
char* strcpy(char*t,const char*s);
size_t strlen(const char*s);
int main()
{
    char c[]="ala ma kota";
    char p[90]; //rozmiar!!
    printf("%zu",strlen(c));
    return 0;
}
size_t strlen(const char*s){
    int len=0;
    while((*s++)){
        ++len;
    }
    return len;
}
#include <stdio.h>
#include <stdlib.h>
char* strncpy(char* dest,const char* src,size_t n);
int main()
{
    char c[]="ala ma kota";
    char p[90]; //rozmiar!!
    strncpy(p,c,6);
    printf("%s",p);
    return 0;
}
char* strncpy(char* dest,const char* src,size_t n){
   for(int i=0;i<n;i++){
    *(dest+i) = *(src+i);
   }
   return dest;
   }
#include <stdio.h>
#include <stdlib.h>
char* strncat(char* dest,const char* src,size_t n);
size_t strlen(const char*s);
int main()
{
    char c[]="ala ma kota";
    char p[90]; //rozmiar!!
    strlen(c);
    strncat(p,strlen(c),2);
    printf("%s",p);
    return 0;
}
char* strncat(char* dest,const char* src,size_t n){
   for(int i=0+n;i<*src;i++){
    *(dest+i) = *(src+i);
   }
   return dest;
   }
size_t strlen(const char*s){
    int len=0;
    while((*s++)){
        ++len;
    }
    return len;
}
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
size_t strlen(const char*s);
char *strcat (char* strTo, const char* strFrom);
int main()
{
    char str[30] = "jakis napis ";   // kopiujemy napis do tablicy
    const char *strFrom = "test";   // zapisujemy wskaznik na napis

    printf("%s",strcat (str, strFrom));

    return 0;
}
#include <stdio.h>
#include <string.h>
int main()
{
char s[20] = "katarzyna i "; // konieczna inicjalizacja
printf("%s\n",strncat(s, "ala ma kota", 9));
}

