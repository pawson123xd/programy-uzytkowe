#include <stdio.h>
#include <stdlib.h>
char* strcpy(char*t,const char*s);
int main()
{
    char c[]="ala ma kota";
    char p[90]; //rozmiar!!
    strcpy(p,c);
    printf("%s",p);
    return 0;
}
char* strcpy(char *t,const char *s){
    char *p = t;
    while((*t++ =*s++));
    return p;}
#include <stdio.h>
#include <stdlib.h>
char* strcpy(char*t,const char*s);
size_t strlen(const char*s);
int main()
{
    char c[]="ala ma kota";
    char p[90]; //rozmiar!!
    printf("%zu",strlen(c));
    return 0;
}
size_t strlen(const char*s){
    int len=0;
    while((*s++)){
        ++len;
    }
    return len;
}
#include <stdio.h>
#include <stdlib.h>
char* strncpy(char* dest,const char* src,size_t n);
int main()
{
    char c[]="ala ma kota";
    char p[90]; //rozmiar!!
    strncpy(p,c,6);
    printf("%s",p);
    return 0;
}
char* strncpy(char* dest,const char* src,size_t n){
   for(int i=0;i<n;i++){
    *(dest+i) = *(src+i);
   }
   return dest;
   }
#include <stdio.h>
#include <stdlib.h>
char* strncat(char* dest,const char* src,size_t n);
size_t strlen(const char*s);
int main()
{
    char c[]="ala ma kota";
    char p[90]; //rozmiar!!
    strlen(c);
    strncat(p,strlen(c),2);
    printf("%s",p);
    return 0;
}
char* strncat(char* dest,const char* src,size_t n){
   for(int i=0+n;i<*src;i++){
    *(dest+i) = *(src+i);
   }
   return dest;
   }
size_t strlen(const char*s){
    int len=0;
    while((*s++)){
        ++len;
    }
    return len;
}

